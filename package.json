{
  "name": "@cdfzo/cli",
  "version": "0.1.0",
  "description": "A command-line interface builder",
  "author": "cdfzo",
  "license": "MIT",
  "homepage": "https://github.com/cdfzo/cli#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "cli",
    "args",
    "argument",
    "parser",
    "command"
  ],
  "scripts": {
    "setup": "bun i",
    "dev": "examples/index.ts",
    "lint": "bunx --bun eslint .",
    "test": "bun test",
    "build": "bun build src/index.ts --outfile dist/index.js --minify && bunx --bun tsc src/index.ts --outFile dist/index -d --module nodenext --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdfzo/cli.git"
  },
  "bugs": {
    "url": "https://github.com/cdfzo/cli/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "bun-types": "^1.0.0",
    "eslint": "^8.56.0"
  },
  "peerDependencies": {
    "typescript": "^5.3.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
